fun main() {
    val calificacion1 = 8
    val calificacion2 = 8
    val calificacion3 = 10

    val promedio = (calificacion1 + calificacion2 + calificacion3) / 3.0

    println("Calificación 1: $calificacion1")
    println("Calificación 2: $calificacion2")
    println("Calificación 3: $calificacion3")
    println("Promedio: $promedio")
}
